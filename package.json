{
  "name": "carsclement",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4.3.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1.8.0",
    "@sap/cds-dk": "^8"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        }
      },
      "[production]": {
        "db": {
          "kind": "hana"
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  },
  "scripts": {
    "start": "cds-serve"
  }
}
